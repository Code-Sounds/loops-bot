    "loops": "dist/index.js"
  },
  "engines": {
    "node": ">= 16.6.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/node": "^16.4.3",
    "prettier": "^2.3.2",
    "ts-node": "^10.1.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@discordjs/opus": "^0.6.0",
    "discord.js": "12.0.0",
    "dotenv": "^10.0.0",
    "ffmpeg-binaries": "3.2.2-3",
    "ffmpeg-static": "^4.4.0",
    "opusscript": "^0.0.8",
    "ytdl-core": "^4.9.1"
  }
